{
  "name": "putParty",
  "license": "Apache-2.0",
  "version": "0.0.2",
  "author": "Michael DeCorte",
  "description": "For test",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelDeCorte/LambdaExample.git"
  },
  "bugs": {
    "url": "https://github.com/MichaelDeCorte/LambdaExample/issues"
  },
  "homepage": "https://github.com/MichaelDeCorte/LambdaExample#readme",
  "scripts": {
    "lint": "eslint src/*.js test/*.js",
    "unit": "jest unit --no-color --coverage",
    "aws": "jest aws --no-color --coverage",
    "service": "jest service --no-color --coverage",
    "dependencies": "bundle-deps",
    "package": "serverless package"
  },
  "devDependencies": {
    "aws-sdk": "^2.225.1",
    "aws-sdk-mock": "^1.7.0",
    "bundle-deps": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "jest": "^22.4.3",
    "jest-each": "^0.3.1",
    "npm": "^5.8.0",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "log-driver": "^1.2.7",
    "promise": "^8.0.1",
    "string-hash": "^1.1.3"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "roots": [
      "src",
      "test"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.terraform/"
    ],
    "modulePaths": [
      "./src",
      "./test",
      "./node_modules"
    ],
    "testEnvironment": "node"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 5
    },
    "extends": [
      "airbnb-base",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "import",
      "jest"
    ],
    "rules": {
      "indent": [
        "error",
        4,
        {
          "FunctionDeclaration": {
            "parameters": "first"
          },
          "FunctionExpression": {
            "parameters": "first"
          },
          "CallExpression": {
            "arguments": "first"
          },
          "ArrayExpression": "first",
          "ObjectExpression": "first",
          "ImportDeclaration": "first",
          "ignoreComments": true
        }
      ],
      "max-len": [
        "warn"
      ],
      "prefer-const": [
        "off"
      ],
      "quote-props": [
        "error",
        "always"
      ],
      "no-console": "off",
      "no-multi-spaces": "off",
      "no-trailing-spaces": "off",
      "object-shorthand": "off",
      "prefer-template": "off",
      "function-paren-newline": "off",
      "padded-block": "off",
      "comma-dangle": "off",
      "prefer-destructuring": "off"
    },
    "env": {
      "node": true
    }
  },
  "bundledDependencies": [
    "log-driver",
    "promise",
    "string-hash"
  ]
}