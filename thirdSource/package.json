{
  "name": "putParty",
  "version": "0.0.1",
  "author": "Michael DeCorte",
  "private": true,
  "description": "For test",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelDeCorte/LambdaExample.git"
  },
  "bugs": {
    "url": "https://github.com/MichaelDeCorte/LambdaExample/issues"
  },
  "homepage": "https://github.com/MichaelDeCorte/LambdaExample#readme",
  "dependencies": {},
  "devDependencies": {
    "@angular/cli": "^6.0.8",
    "aws-sdk": "^2.241.1",
    "aws-sdk-mock": "^1.7.0",
    "bundle-deps": "^1.0.0",
    "download-size": "^1.3.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "jest": "^22.4.3",
    "jest-each": "^0.3.1",
    "jsonlint": "^1.6.3",
    "madge": "^3.0.1",
    "ng": "0.0.0-rc6",
    "npm": "^6.2.0",
    "supertest": "^3.1.0"
  },
  "bundledDependencies": [],
  "scripts": {
    "lint": "eslint */src/*.js */test/*.js",
    "unit": "jest unit --no-color --coverage",
    "aws": "jest aws --no-color --coverage",
    "service": "jest service --no-color --coverage",
    "dependencies": "bundle-deps",
    "download-size": "download-size",
    "jsonlint": "jsonlint"
  },
  "logger": {
    "default": "trace",
    "[common]src/guid.js": "trace",
    "[party]": "trace"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "roots": [
      "party/src",
      "party/test"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.terraform/"
    ],
    "modulePaths": [
      "./party/src",
      "./party/test",
      "./node_modules"
    ],
    "testEnvironment": "node"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 5
    },
    "extends": [
      "airbnb-base",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "import",
      "jest"
    ],
    "rules": {
      "indent": [
        "error",
        4,
        {
          "FunctionDeclaration": {
            "parameters": "first"
          },
          "FunctionExpression": {
            "parameters": "first"
          },
          "CallExpression": {
            "arguments": "first"
          },
          "ArrayExpression": "first",
          "ObjectExpression": "first",
          "ImportDeclaration": "first",
          "ignoreComments": true
        }
      ],
      "max-len": [
        "warn"
      ],
      "prefer-const": [
        "off"
      ],
      "quote-props": [
        "error",
        "always"
      ],
      "newline-per-chained-call": "off",
      "no-console": "off",
      "no-multi-spaces": "off",
      "no-trailing-spaces": "off",
      "object-shorthand": "off",
      "prefer-template": "off",
      "function-paren-newline": "off",
      "padded-block": "off",
      "comma-dangle": "off",
      "import/no-extraneous-dependencies": "off",
      "arrow-body-style": [
        "error",
        "always"
      ],
      "prefer-destructuring": "off",
      "no-underscore-dangle": "off",
      "curly": "off",
      "no-plusplus": "off"
    },
    "env": {
      "node": true
    }
  }
}
