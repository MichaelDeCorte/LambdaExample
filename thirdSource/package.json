{
  "name": "putParty",
  "version": "0.0.1",
  "author": "Michael DeCorte",
  "private": true,
  "description": "For test",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelDeCorte/LambdaExample.git"
  },
  "bugs": {
    "url": "https://github.com/MichaelDeCorte/LambdaExample/issues"
  },
  "homepage": "https://github.com/MichaelDeCorte/LambdaExample#readme",
  "dependencies": {
    "amazon-cognito-identity-js-with-fetch": "^3.0.5",
    "common": "file:common",
    "common.test": "file:common.test",
    "joi": "^14.3.1",
    "log-driver": "^1.2.7",
    "methodRouter": "file:methodRouter",
    "npm-install-all": "^1.1.21",
    "party": "file:party",
    "promise": "^8.0.2",
    "string-hash": "^1.1.3",
    "website": "file:website"
  },
  "devDependencies": {
    "@angular/cli": "^7.3.3",
    "amazon-cognito-identity-js": "^3.0.7",
    "aws-sdk": "^2.409.0",
    "aws-sdk-mock": "^1.7.0",
    "bundle-deps": "^1.0.0",
    "download-size": "^1.3.2",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "jest": "21.1.0",
    "jest-each": "^0.3.1",
    "jsdom": "11.11.0",
    "jsonlint": "^1.6.3",
    "madge": "^3.0.1",
    "ng": "0.0.0-rc6",
    "node-fetch": "^2.3.0",
    "npm": "^6.8.0",
    "superagent-logger": "^1.1.0",
    "supertest": "^3.4.2",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  },
  "scripts": {
    "lint": "eslint */src/*.js */test/*.js",
    "unit": "jest unit --no-color --coverage",
    "aws": "jest aws --no-color --coverage",
    "service": "jest service --no-color --coverage",
    "dependencies": "bundle-deps",
    "download-size": "download-size",
    "jsonlint": "jsonlint"
  },
  "logger": {
    "default": "error"
  },
  "loggerExample": {
    "default": "debug",
    "[common]src/guid.js": "debug",
    "[party]": "debug"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "roots": [
      "party/src",
      "party/test"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.terraform/"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 5
    },
    "extends": [
      "airbnb-base",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "import",
      "jest"
    ],
    "rules": {
      "indent": [
        "error",
        4,
        {
          "FunctionDeclaration": {
            "parameters": "first"
          },
          "FunctionExpression": {
            "parameters": "first"
          },
          "CallExpression": {
            "arguments": "first"
          },
          "ArrayExpression": "first",
          "ObjectExpression": "first",
          "ImportDeclaration": "first",
          "ignoreComments": true
        }
      ],
      "max-len": [
        "warn"
      ],
      "prefer-const": [
        "off"
      ],
      "quote-props": [
        "error",
        "always"
      ],
      "newline-per-chained-call": "off",
      "no-console": "off",
      "no-multi-spaces": "off",
      "no-trailing-spaces": "off",
      "object-shorthand": "off",
      "prefer-template": "off",
      "function-paren-newline": "off",
      "padded-block": "off",
      "comma-dangle": "off",
      "import/no-extraneous-dependencies": "off",
      "import/no-unresolved": "off",
      "nonblock-statement-body-position": "off",
      "operator-linebreak": "off",
      "arrow-body-style": [
        "error",
        "always"
      ],
      "prefer-destructuring": "off",
      "no-underscore-dangle": "off",
      "curly": "off",
      "no-plusplus": "off"
    },
    "env": {
      "node": true
    }
  },
  "bundleDependencies": [],
  "main": "index.js",
  "keywords": []
}
